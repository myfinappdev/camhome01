{
	"name": "copycamhomefiles_v1",
	"properties": {
		"description": "new",
		"activities": [
			{
				"name": "CAMHOMEFILECOPY",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.1:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "CAM_HOME01/*.jpg"
					},
					{
						"name": "Destination",
						"value": "camhome/CAM_HOME01/IMAGES/@{'<FileName>'}"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "FtpReadSettings",
							"recursive": true,
							"useBinaryTransfer": false,
							"deleteFilesAfterCompletion": true,
							"disableChunking": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						}
					},
					"enableStaging": false,
					"parallelCopies": 10,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Warning",
							"enableReliableLogging": true
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "camahomeoutstore",
								"type": "LinkedServiceReference"
							},
							"path": {
								"value": "@concat('camhome/CAM_HOME01/IMAGES/',formatDateTime(utcNow(),'YYYYMMDD'))",
								"type": "Expression"
							}
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Camhomeinputs",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Camhomeout",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "@concat('camhome01_image_',formatDateTime(utcNow(),'yyyMMdd_HHmmss'),'.jpg')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"annotations": [
			"SHIJU JOY"
		],
		"lastPublishTime": "2022-08-25T22:40:27Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}