					},
					"enableStaging": false,
					"parallelCopies": 5
				},
				"inputs": [
					{
						"referenceName": "Camhomeinputs",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "test",
						"type": "DatasetReference"
					}
				]
			}
		],
		"concurrency": 10,
		"annotations": []
	}
